{
  "name": "nodejstemplete",
  "version": "0.0.1",
  "description": "",
  "author": "VFA",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && yarn run generate-typings && tsc  -p tsconfig.build.json  && copyfiles -u 1 ./src/**/*.graphql dist/",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start": "node dist/main.js",
    "format": "prettier --write \"./src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint-fix": "tslint --fix -p tsconfig.json -c tslint.json",
    "pre-commit": "yarn run lint-staged",
    "generate-typings": "ts-node src/vendors/schema/generate-typings.ts",
    "test": "jest --detectOpenHandles --runInBand --runInBand --forceExit  --verbose ",
    "gen-query-graphql": "ts-node __tests__/utils/gqlGenerator.ts",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./__tests__/jest-e2e.json --verbose --runInBand --forceExit",
    "test:e2e:file": "npm run test:e2e -t",
    "start-local-docker": "export NODE_ENV=local && docker-compose -f ./docker-compose.yml up --build -d mysql-graphql",
    "stop-local-docker": "docker-compose -f ./docker-compose.yml down"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^6.5.3",
    "@nestjs/core": "^6.5.3",
    "@nestjs/graphql": "^6.4.2",
    "@nestjs/jwt": "^6.0.0",
    "@nestjs/passport": "^6.0.0",
    "@nestjs/platform-express": "^6.5.3",
    "@nestjs/platform-fastify": "^6.5.3",
    "@nestjs/typeorm": "^6.1.3",
    "@types/bull": "^3.10.6",
    "add": "^2.0.6",
    "apollo-server-express": "^2.8.1",
    "aws-sdk": "^2.515.0",
    "base64-img": "^1.0.4",
    "bcrypt": "^4.0.1",
    "bestzip": "^2.1.4",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "csv-split-stream": "^1.0.2",
    "dotenv": "^8.2.0",
    "graphql": "^14.4.2",
    "graphql-request": "^1.8.2",
    "graphql-tools": "^5.0.0-rc.1",
    "graphql-type-json": "^0.3.0",
    "lodash": "^4.17.11",
    "merge-graphql-schemas": "^1.7.6",
    "moment": "^2.24.0",
    "mongodb": "^3.2.5",
    "mysql": "^2.17.1",
    "nest-router": "^1.0.9",
    "nest-schedule": "^0.6.3",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.4.0",
    "typeorm": "^0.3.0-alpha.23",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.10",
    "@nestjs/config": "^0.4.0",
    "@nestjs/testing": "^6.5.3",
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.16.1",
    "@types/hapi__joi": "^16.0.12",
    "@types/jest": "^24.0.16",
    "@types/lodash": "^4.14.149",
    "@types/node": "^11.13.18",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.8",
    "apollo-server-testing": "^2.8.1",
    "copyfiles": "^2.2.0",
    "faker": "^4.1.0",
    "gql-generator": "^1.0.12",
    "jest": "^24.7.1",
    "lint-staged": "^10.0.0-beta.14",
    "mocker-data-generator": "^2.6.6",
    "nodemon": "^1.18.11",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.20.1",
    "typescript": "^3.5.3",
    "xmlhttprequest": "^1.8.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src/",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint-staged"
    }
  }
}
