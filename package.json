{
  "name": "nodejstemplete",
  "version": "0.0.1",
  "description": "",
  "author": "VFA",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && yarn run generate-typings:stamp && tsc  -p tsconfig.build.json  && copyfiles -u 1 ./src/**/*.graphql dist/",
    "start:dev": "nodemon",
    "start:prod": "node dist/main.js",
    "postinstall": "npm i yarn && npm run prestart:prod",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start": "node dist/main.js",
    "format": "prettier --write \"./src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "pre-commit": "yarn run lint-staged",
    "generate-typings:booking": "ts-node src/vendors/schema/generate-typings.ts",
    "generate-typings:stamp": "ts-node src/vendors/schema/generate-typings-stamp.ts",
    "test": "jest --detectOpenHandles --runInBand --runInBand --forceExit  --verbose ",
    "gen-query-graphql": "ts-node __tests__/utils/gqlGenerator.ts",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./__tests__/jest-e2e.json --verbose --runInBand --forceExit",
    "test:e2e:file": "npm run test:e2e -t",
    "start-docker": "export NODE_ENV=docker && docker-compose -f ./docker-compose.yml up --build -d",
    "start-local-docker": "export NODE_ENV=local && docker-compose -f ./docker-compose.dev.yml up --build",
    "stop-local-docker": "docker-compose -f ./docker-compose.dev.yml down"
  },
  "dependencies": {
    "@nestjs/common": "^7.4.4",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.4.4",
    "@nestjs/graphql": "^7.6.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.4.4",
    "@nestjs/typeorm": "^7.1.3",
    "@types/bull": "^3.10.6",
    "apollo-server-express": "^2.17.0",
    "aws-sdk": "^2.515.0",
    "base64-img": "^1.0.4",
    "bcrypt": "^4.0.1",
    "bestzip": "^2.1.4",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.12.2",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "graphql": "14.7.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-request": "^1.8.2",
    "graphql-type-json": "^0.3.2",
    "joi": "^17.2.1",
    "lodash": "^4.17.11",
    "mysql": "^2.17.1",
    "nest-router": "^1.0.9",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "typeorm": "^0.2.25",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@graphql-tools/load-files": "^6.2.1",
    "@graphql-tools/merge": "^6.2.1",
    "@nestjs/testing": "^7.4.4",
    "@types/bcrypt": "^3.0.0",
    "@types/joi": "^14.3.4",
    "@types/lodash": "^4.14.149",
    "@types/supertest": "^2.0.8",
    "apollo-server-testing": "^2.8.1",
    "copyfiles": "^2.2.0",
    "faker": "^4.1.0",
    "gql-generator": "^1.0.12",
    "husky": "^3.1.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.0.0-beta.14",
    "mocker-data-generator": "^2.6.6",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.20.1",
    "typescript": "^4.0.2",
    "xmlhttprequest": "^1.8.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src/",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint-staged"
    }
  }
}
