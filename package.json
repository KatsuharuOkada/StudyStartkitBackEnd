{
  "name": "nodejstemplete",
  "version": "0.0.1",
  "description": "This is project Graphql template",
  "author": "VFA",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json  && copyfiles -u 1 ./src/**/*.graphql dist/",
    "start:dev": "tsnd src/main.ts",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start": "node dist/main.js",
    "genenate-typings": "ts-node src/vendors/schema/generate-typings.ts",
    "test": "jest --detectOpenHandles --runInBand --forceExit  --verbose ",
    "gen-query-graphql": "ts-node __tests__/utils/gqlGenerator.ts",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "DOTENV_CONFIG_PATH=env/.env.test jest --config ./__tests__/jest-e2e.json --verbose --runInBand",
    "test:e2e:cov": "DOTENV_CONFIG_PATH=env/.env.test jest --config ./jest-cov-e2e.json --verbose --runInBand --coverage",
    "test:e2e:file": "yarn run test:e2e -t",
    "start-docker": "export NODE_ENV=docker && docker-compose -f ./docker-compose.yml up --build -d",
    "start-local-docker": "export NODE_ENV=local && docker-compose -f ./docker-compose.dev.yml up --build",
    "stop-local-docker": "docker-compose -f ./docker-compose.dev.yml down",
    "prepare": "husky install",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migrations:run": "yarn run typeorm migration:run -- -d ./typeOrm.config.ts",
    "migration:generate": "yarn run typeorm -- -d ./typeOrm.config.ts migration:generate ./src/migrations/$npm_config_name",
    "migration:create": "yarn run typeorm -- migration:create ./src/migrations/$npm_config_name",
    "migration:revert": "yarn run typeorm -- -d ./typeOrm.config.ts migration:revert",
    "generate-code": "plop && yarn run genenate-typings"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.1.0",
    "@nestjs/common": "^9.1.1",
    "@nestjs/core": "^9.1.1",
    "@nestjs/graphql": "^10.1.2",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.1.1",
    "@nestjs/typeorm": "^9.0.1",
    "@types/bull": "^3.15.9",
    "@types/jest": "^29.2.1",
    "apollo-server-core": "^3.10.2",
    "apollo-server-express": "^3.10.2",
    "aws-sdk": "^2.1221.0",
    "base64-img": "^1.0.4",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.2",
    "graphql": "^16.6.0",
    "joi": "^17.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mysql2": "^2.3.3",
    "passport": "^0.6.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "plop": "^3.1.1",
    "redis": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "source-map-support": "^0.5.21",
    "ts-morph": "^16.0.0",
    "typeorm": "^0.3.10",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@graphql-tools/load-files": "^6.6.1",
    "@graphql-tools/merge": "^8.3.6",
    "@nestjs/config": "^2.2.0",
    "@nestjs/testing": "^9.1.1",
    "@types/bcrypt": "^5.0.0",
    "@types/joi": "^17.2.3",
    "@types/lodash": "^4.14.185",
    "@types/supertest": "^2.0.12",
    "apollo-server-testing": "^2.25.3",
    "copyfiles": "^2.4.1",
    "faker": "^6.6.6",
    "gql-generator": "^1.0.17",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "lint-staged": "^13.0.3",
    "mocker-data-generator": "^2.12.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.8.3",
    "xmlhttprequest": "^1.8.0",
    "yarn-upgrade-all": "^0.7.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src/",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run pretty-quick --staged"
    }
  }
}
