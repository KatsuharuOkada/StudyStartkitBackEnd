{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "setupFilesAfterEnv": ["./__tests__/setup/jest.setup.ts"],
  "testRegex": ".*\\.e2e.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "coverageDirectory": "./coverage-e2e",
  "collectCoverageFrom": ["src/*.ts", "src/**/*.ts", "src/**/**/*.ts", "src/**/**/**/*.ts"]
}
